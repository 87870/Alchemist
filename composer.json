{
  "name": "serri/alchemist",
  "version": "1.0.2",
  "description": "The JSON Revolution for Laravel, a simple, fast, and elegant alternative to Laravel JSON Resource. ",
  "license": "MIT",
  "authors": [
    {
      "name": "Adnan Serri",
      "email": "serri.adnan.47@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "orchestra/testbench": "^10.2.1"
  },
  "autoload": {
    "psr-4": {
      "Serri\\Alchemist\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Serri\\Alchemist\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Serri\\Alchemist\\Providers\\AlchemistServiceProvider"
      ]
    }
  }
}
